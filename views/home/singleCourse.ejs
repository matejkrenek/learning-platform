<%- include("../partials/navbar") %>
<h1>courses <%= course.title %></h1>
<table>
    <thead>
        <tr>
            <th>Pořadí</th>
            <th>Název</th>
            <th>Popis</th>
            <th>cta</th>
        </tr>
    </thead>
    <tbody>
        <% course.sections.forEach(section => { %>
            <tr>
                <td>1</td>
                <td><%= section.title %></td>
                <td><%= section.description %></td>
                <td><a href="/home/course/section/<%= section._id %>">Zobrazit lekce</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>