<h1>users</h1>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Jméno</th>
            <th>Příjmení</th>
            <th>E-mailová adresa</th>
            <th>Pravomoce</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user._id %></td>
                <td><%= user.name.first %></td>
                <td><%= user.name.last %></td>
                <td><%= user.email %></td>
                <td><%= user.role %></td>
            </tr>
        <% }) %>
    </tbody>
</table>